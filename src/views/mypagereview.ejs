  <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
  <script src="/js/mypagereview.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://kit.fontawesome.com/fd3102c9a3.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="../css/mypagereview.css" />
  <title>mypagereview</title>
</head>


<body style="background-color: #edf5e1">
  <%- include('nav.ejs') %>
  <div class="side">
    <ul class="sidebar">
      <li>
        <a href="/auth/edit">
          <i class="fa-solid fa-circle-user fa-3x">
            <div class="circle"></div>
          </i>
          <div class="title">개인정보 수정</div>
        </a>
      </li>
      <li>
        <a href="/article/mypagearticle">
          <i class="fa-solid fa-list fa-3x">
            <div class="circle"></div>
          </i>
          <div class="title">게시글 관리</div>
        </a>
      </li>
      <li>
        <a href="/comment/mypagecomment">
          <i class="fa-solid fa-comment fa-3x">
            <div class="circle"></div>
          </i>
          <div class="title">댓글 관리</div>
        </a>
      </li>
      <li>
        <a href="/review/mypagereview">
          <i class="fa-solid fa-pencil fa-3x">
            <div class="circle"></div>
          </i>
          <div class="title">리뷰내역</div>
        </a>
      </li>
      <li>
        <a href="/auth/withdrawal">
          <i class="fa-solid fa-user-xmark fa-3x">
            <div class="circle"></div>
          </i>
          <div class="title">회원탈퇴</div>
        </a>
      </li>
      <div class="background"></div>
    </ul>
  </div>
    <div class="container">
      <div class="boardWrap">
        <div class="boardTitle">
          <strong>리뷰</strong>
        </div>
        <div class="boardListWrap">
          <div class="boardList">
            <div class="top">
              <div class="id">번호</div>
              <div class="title">제목</div>
              <div class="date">작성일</div>
              <div class="count">조회</div>
            </div>
          </div>
          <div class="pagination"></div>
        </div>
      </div>
    </div>

  </body>
  <%- include('footer.ejs') %>

